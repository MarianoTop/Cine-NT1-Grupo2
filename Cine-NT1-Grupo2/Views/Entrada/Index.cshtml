@model IEnumerable<Cine_NT1_Grupo2.Models.Entrada>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
            <th>
                @Html.DisplayNameFor(model => model.Funcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Asiento)
            </th>
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Funcion.Pelicula.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asiento.Fila)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EntradaId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EntradaId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EntradaId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
